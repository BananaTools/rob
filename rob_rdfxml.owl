    <rdf:Description>
        <swrla:isRuleEnabled rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</swrla:isRuleEnabled>
        <rdfs:comment>If a saga S has associated a Tag T, then all the books of the saga S will be associated withe the Tag T</rdfs:comment>
        <rdfs:label>From saga_tag to book_tag</rdfs:label>
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Imp"/>
        <swrl:body>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                <rdf:first>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                        <swrl:classPredicate rdf:resource="http://www.semanticweb.org/rob#Book_Serie"/>
                        <swrl:argument1 rdf:resource="http://www.semanticweb.org/rob#s"/>
                    </rdf:Description>
                </rdf:first>
                <rdf:rest>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                        <rdf:first>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                                <swrl:classPredicate rdf:resource="http://www.semanticweb.org/rob#Book"/>
                                <swrl:argument1 rdf:resource="http://www.semanticweb.org/rob#b"/>
                            </rdf:Description>
                        </rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                <rdf:first>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                                        <swrl:classPredicate rdf:resource="http://www.semanticweb.org/rob#Tag"/>
                                        <swrl:argument1 rdf:resource="http://www.semanticweb.org/rob#t"/>
                                    </rdf:Description>
                                </rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                        <rdf:first>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                                                <swrl:propertyPredicate rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#isMemberOf"/>
                                                <swrl:argument1 rdf:resource="http://www.semanticweb.org/rob#b"/>
                                                <swrl:argument2 rdf:resource="http://www.semanticweb.org/rob#s"/>
                                            </rdf:Description>
                                        </rdf:first>
                                        <rdf:rest>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                                <rdf:first>
                                                    <rdf:Description>
                                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                                                        <swrl:propertyPredicate rdf:resource="http://www.semanticweb.org/rob#saga_tag"/>
                                                        <swrl:argument1 rdf:resource="http://www.semanticweb.org/rob#s"/>
                                                        <swrl:argument2 rdf:resource="http://www.semanticweb.org/rob#t"/>
                                                    </rdf:Description>
                                                </rdf:first>
                                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                            </rdf:Description>
                                        </rdf:rest>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </rdf:rest>
            </rdf:Description>
        </swrl:body>
        <swrl:head>
