<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/rob"
     xml:base="http://www.semanticweb.org/rob"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:rob="http://www.semanticweb.org/rob#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:collectionentity="http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#"
     xmlns:cpannotationschema="http://www.ontologydesignpatterns.org/schemas/cpannotationschema.owl#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/rob">
        <owl:imports rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled -->

    <owl:AnnotationProperty rdf:about="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled"/>
    


    <!-- http://www.w3.org/2000/01/rdf-schema#label -->

    <rdf:Description rdf:about="http://www.w3.org/2000/01/rdf-schema#label">
        <rdfs:label xml:lang="en">Recommendation Ontology Book</rdfs:label>
    </rdf:Description>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/rob#book_similar_to -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/rob#book_similar_to">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/rob#Book"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/rob#Book"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/rob#book_tag -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/rob#book_tag">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/rob#Book"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/rob#Tag"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/rob#favourite_book -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/rob#favourite_book">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/rob#likes_book"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/rob#User"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/rob#Book"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/rob#favourite_genre -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/rob#favourite_genre">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/rob#likes_genre"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/rob#User"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/rob#Literary_Genre"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="http://www.semanticweb.org/rob#favourite_book"/>
            <rdf:Description rdf:about="http://www.semanticweb.org/rob#genre"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/rob#genre -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/rob#genre">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/rob#Book"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/rob#Literary_Genre"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/rob#likes_book -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/rob#likes_book">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/rob#User"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/rob#Book"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/rob#likes_genre -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/rob#likes_genre">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/rob#User"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/rob#Literary_Genre"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/rob#recommendation -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/rob#recommendation">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/rob#recommendation_for"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/rob#User"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/rob#Books_Recommendation"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/rob#recommendation_for -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/rob#recommendation_for">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/rob#Books_Recommendation"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/rob#User"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/rob#saga_tag -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/rob#saga_tag">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/rob#Book_Serie"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/rob#Tag"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/rob#user_similar_to -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/rob#user_similar_to">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/rob#User"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/rob#User"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/rob#user_tag -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/rob#user_tag">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/rob#User"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/rob#Tag"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/rob#was_written_by -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/rob#was_written_by">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/rob#wrote"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/rob#wrote -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/rob#wrote">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/rob#Author"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/rob#Book"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/rob#copies_sold -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/rob#copies_sold">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/rob#Book"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/rob#published_in -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/rob#published_in">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/rob#Book"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/Agent -->

    <owl:Class rdf:about="http://purl.org/dc/terms/Agent"/>
    


    <!-- http://www.semanticweb.org/rob#Author -->

    <owl:Class rdf:about="http://www.semanticweb.org/rob#Author">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/rob#wrote"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/rob#Book"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/rob#User"/>
        <rdfs:comment xml:lang="en">Subclass of User with comprises anyone who has written a book. In any case there may also be recommendations for authors, because the recommendations are for all the users, not only for the readers.</rdfs:comment>
        <rdfs:label xml:lang="en">Author</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/rob#BestSeller -->

    <owl:Class rdf:about="http://www.semanticweb.org/rob#BestSeller">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/rob#Book"/>
        <rdfs:comment xml:lang="en">In this ontology we have done a semplification, we have assumed that book is a bestseller if it sold at least 10.000 copies this in order to not implement the question of time.</rdfs:comment>
        <rdfs:label xml:lang="en">BestSeller</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/rob#Book -->

    <owl:Class rdf:about="http://www.semanticweb.org/rob#Book">
        <rdfs:comment xml:lang="en">The focus of this ontology is the book, in fact he is the subject of a reccomendation</rdfs:comment>
        <rdfs:label xml:lang="en">Book</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/rob#Book_Serie -->

    <owl:Class rdf:about="http://www.semanticweb.org/rob#Book_Serie">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection"/>
        <rdfs:comment xml:lang="en">Collection of books  that are formally identified together as a group. This in according to the pattern Submissions:CollectionEntity. If there are 2 books in a series it&apos;s called Duology, with 3 books it&apos;s called Trilogy and so on.</rdfs:comment>
        <rdfs:label xml:lang="en">Book Serie</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/rob#Books_Recommendation -->

    <owl:Class rdf:about="http://www.semanticweb.org/rob#Books_Recommendation">
        <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/collectionentity.owl#Collection"/>
        <rdfs:comment xml:lang="en">A recommendation of books for a specific system user. In particulary a recommendation is a collection of books according to the pattern Submissions:CollectionEntity. The is one recommendation for each user of the system.</rdfs:comment>
        <rdfs:label xml:lang="en">Books Recommendation</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/rob#Crime -->

    <owl:Class rdf:about="http://www.semanticweb.org/rob#Crime">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/rob#Fiction"/>
        <rdfs:comment xml:lang="en">Fiction about a crime, how the criminal gets caught and serve time, and the repercussions of the crime</rdfs:comment>
        <rdfs:label xml:lang="en">Crime genre</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/rob#Famous_Author -->

    <owl:Class rdf:about="http://www.semanticweb.org/rob#Famous_Author">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/rob#wrote"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/rob#BestSeller"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/rob#User"/>
        <rdfs:comment xml:lang="en">An author who wrote a bestseller.</rdfs:comment>
        <rdfs:label xml:lang="en">Famous Author</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/rob#Fantasy -->

    <owl:Class rdf:about="http://www.semanticweb.org/rob#Fantasy">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/rob#Fiction"/>
        <rdfs:comment xml:lang="en">Fantasy is a genre of speculative fiction set in a fictional universe, often inspired by real world myth and folklore.</rdfs:comment>
        <rdfs:label xml:lang="en">Fantasy genre</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/rob#Fiction -->

    <owl:Class rdf:about="http://www.semanticweb.org/rob#Fiction">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/rob#Literary_Genre"/>
        <rdfs:comment xml:lang="en">Narrative form not based strictly on history or fact.</rdfs:comment>
        <rdfs:label xml:lang="en">Fiction (Literary Genre)</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/rob#Horror -->

    <owl:Class rdf:about="http://www.semanticweb.org/rob#Horror">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/rob#Fiction"/>
        <rdfs:comment xml:lang="en">Horror is a genre of speculative fiction which is intended to frighten, scare, disgust, or startle its readers by inducing feelings of horror and terror.</rdfs:comment>
        <rdfs:label xml:lang="en">Horror genre</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/rob#Literary_Genre -->

    <owl:Class rdf:about="http://www.semanticweb.org/rob#Literary_Genre">
        <rdfs:comment xml:lang="en">Using the term literary genre, we make it clear that we&apos;re talking about books and writing. There are many genres, in this ontology have been reported only a few.</rdfs:comment>
        <rdfs:label xml:lang="en">Literary Genre</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/rob#Nonfiction -->

    <owl:Class rdf:about="http://www.semanticweb.org/rob#Nonfiction">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/rob#Literary_Genre"/>
        <rdfs:comment xml:lang="en">Content that purports in good faith to represent truth and accuracy regarding information, events, or people.</rdfs:comment>
        <rdfs:label xml:lang="en">Nonfiction genre</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/rob#Tag -->

    <owl:Class rdf:about="http://www.semanticweb.org/rob#Tag">
        <rdfs:comment xml:lang="en">Tags, in this ontology, can be used to find common points between users, between books or between users and books</rdfs:comment>
        <rdfs:label xml:lang="en">Tag</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/rob#Thriller -->

    <owl:Class rdf:about="http://www.semanticweb.org/rob#Thriller">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/rob#action_thriller"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/rob#crime_thriller"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/rob#psychological_thriller"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/rob#thriller"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/rob#Fiction"/>
        <rdfs:comment xml:lang="en">Thriller is a genre of fiction, having numerous, often overlapping subgenres. Thrillers are characterized and defined by the moods they elicit, giving viewers heightened feelings of suspense, excitement, surprise, anticipation and anxiety.</rdfs:comment>
        <rdfs:label xml:lang="en">Thriller genre</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/rob#User -->

    <owl:Class rdf:about="http://www.semanticweb.org/rob#User">
        <rdfs:subClassOf rdf:resource="http://purl.org/dc/terms/Agent"/>
        <rdfs:comment xml:lang="en">Anyone that use the system is classified as a user. This class includes both readers and writers.</rdfs:comment>
        <rdfs:label xml:lang="en">User</rdfs:label>
    </owl:Class>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Individuals
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/rob#Alice_Fala -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/rob#Alice_Fala">
        <rob:favourite_book rdf:resource="http://www.semanticweb.org/rob#Shining"/>
        <rdfs:comment xml:lang="en">A system user</rdfs:comment>
        <rdfs:label xml:lang="en">Alice Fala</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/rob#Davide_Geco -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/rob#Davide_Geco">
        <rdf:type rdf:resource="http://www.semanticweb.org/rob#User"/>
        <rob:likes_book rdf:resource="http://www.semanticweb.org/rob#Gone_Girl"/>
        <rob:user_similar_to rdf:resource="http://www.semanticweb.org/rob#Emanuele_Podio"/>
        <rdfs:comment xml:lang="en">A system user</rdfs:comment>
        <rdfs:label xml:lang="en">Davide Geco</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/rob#Elisa_Fragola -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/rob#Elisa_Fragola">
        <rob:likes_book rdf:resource="http://www.semanticweb.org/rob#The_Fellowship_of_the_Ring"/>
        <rdfs:comment xml:lang="en">A system user</rdfs:comment>
        <rdfs:label xml:lang="en">Elisa Fragola</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/rob#Emanuele_Podio -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/rob#Emanuele_Podio">
        <rdf:type rdf:resource="http://www.semanticweb.org/rob#User"/>
        <rob:likes_book rdf:resource="http://www.semanticweb.org/rob#The_Dark_Half"/>
        <rob:user_similar_to rdf:resource="http://www.semanticweb.org/rob#Davide_Geco"/>
        <rdfs:comment xml:lang="en">A system user</rdfs:comment>
        <rdfs:label xml:lang="en">Emanuele Podio</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/rob#Gone_Girl -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/rob#Gone_Girl">
        <rob:genre rdf:resource="http://www.semanticweb.org/rob#thriller"/>
        <rdfs:comment xml:lang="en">Gone Girl is a thriller novel in the mystery and crime genres, by the American writer Gillian Flynn.</rdfs:comment>
        <rdfs:label xml:lang="en">Gone Girl</rdfs:label>
        <rdfs:label xml:lang="it">L&apos;amore Bugiardo</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/rob#Harry_Potter -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/rob#Harry_Potter">
        <rdf:type rdf:resource="http://www.semanticweb.org/rob#Book_Serie"/>
        <collectionentity:hasMember rdf:resource="http://www.semanticweb.org/rob#The_Chamber_of_Secrets"/>
        <collectionentity:hasMember rdf:resource="http://www.semanticweb.org/rob#The_Goblet_of_Fire"/>
        <collectionentity:hasMember rdf:resource="http://www.semanticweb.org/rob#The_Prisoner_of_Azkaban"/>
        <collectionentity:hasMember rdf:resource="http://www.semanticweb.org/rob#The_Philosopher&apos;s_Stone"/>
        <rdfs:comment xml:lang="en">Book serie written by J. K. Rowling</rdfs:comment>
        <rdfs:label xml:lang="en">Harry Potter</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/rob#J._K._Rowling -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/rob#J._K._Rowling">
        <rdf:type rdf:resource="http://www.semanticweb.org/rob#Author"/>
        <rdfs:comment xml:lang="en">Joanne Rowling, better known by her pen name J. K. Rowling, is a British author, film producer, television producer, screenwriter, and philanthropist. She is best known for writing the Harry Potter fantasy series, which has won multiple awards and sold more than 500 million copies, becoming the best-selling book series in history.</rdfs:comment>
        <rdfs:label xml:lang="en">Joanne Rowling</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/rob#J._R._R._Tolkien -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/rob#J._R._R._Tolkien">
        <rdf:type rdf:resource="http://www.semanticweb.org/rob#User"/>
        <rob:wrote rdf:resource="http://www.semanticweb.org/rob#The_Fellowship_of_the_Ring"/>
        <rob:wrote rdf:resource="http://www.semanticweb.org/rob#The_Return_of_the_King"/>
        <rob:wrote rdf:resource="http://www.semanticweb.org/rob#The_Two_Towers"/>
        <rdfs:comment xml:lang="en">John Ronald Reuel Tolkien was an English writer, poet, philologist, and academic, who is best known as the author of the classic high fantasy works The Hobbit, The Lord of the Rings, and The Silmarillion.</rdfs:comment>
        <rdfs:label xml:lang="en">John Ronald Reuel Tolkien</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/rob#Luigi_Verdi -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/rob#Luigi_Verdi">
        <rdf:type rdf:resource="http://www.semanticweb.org/rob#User"/>
        <rob:likes_book rdf:resource="http://www.semanticweb.org/rob#The_Red_Pyramid"/>
        <rdfs:comment xml:lang="en">A system user</rdfs:comment>
        <rdfs:label xml:lang="it">Luigi Verdi</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/rob#Mario_Rossi -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/rob#Mario_Rossi">
        <rdf:type rdf:resource="http://www.semanticweb.org/rob#User"/>
        <rob:favourite_book rdf:resource="http://www.semanticweb.org/rob#The_Goblet_of_Fire"/>
        <rdfs:comment xml:lang="en">A system user</rdfs:comment>
        <rdfs:label xml:lang="it">Mario Rossi</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/rob#Matteo_Peso -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/rob#Matteo_Peso">
        <rob:likes_genre rdf:resource="http://www.semanticweb.org/rob#horror"/>
        <rob:user_tag rdf:resource="http://www.semanticweb.org/rob#nostalgic"/>
        <rdfs:comment xml:lang="en">A system user</rdfs:comment>
        <rdfs:label xml:lang="it">Matteo Peso</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/rob#Misery -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/rob#Misery">
        <rob:was_written_by rdf:resource="http://www.semanticweb.org/rob#Stephen_King"/>
        <rdfs:comment xml:lang="en">Book written by Stephen King</rdfs:comment>
        <rdfs:label xml:lang="en">Misery</rdfs:label>
